{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "block/net/pane_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "block/net/pane_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "block/net/pane_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "block/net/pane_side_alt",
        "y": 90
      }
    }
  ]
}
